{
    "name": "nikophil/rector-bug-reproducer",
    "type": "library",
    "authors": [
        {
            "name": "Nicolas PHILIPPE",
            "email": "nikophil@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "nikic/php-parser": "^4.0",
        "symfony/flex": "^2.4",
        "symfony/framework-bundle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Nikophil\\RectorBugReproducer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Utils\\Rector\\": "utils/rector/src",
            "Utils\\Rector\\Tests\\": "utils/rector/tests"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^7.0",
        "symfony/css-selector": "^7.0",
        "symfony/phpunit-bridge": "^7.0",
        "rector/rector": "^1.0"
    }
}
